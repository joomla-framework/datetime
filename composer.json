{
    "name": "joomla/datetime",
    "type": "joomla-package",
    "description": "Joomla DateTime Package",
    "keywords": ["joomla", "framework", "datetime"],
    "homepage": "https://github.com/joomla-framework/datetime",
    "license": "LGPL-2.1+",
    "require": {
        "php": "^5.3.10|~7.0",
        "symfony/translation": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.35|^5.4.3|~6.0",
        "squizlabs/php_codesniffer": "1.*"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\DateTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\DateTime\\Test\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
